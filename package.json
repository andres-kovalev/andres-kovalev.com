{
    "name": "andres-kovalev.com",
    "version": "0.3.2",
    "description": "andres-kovalev.com personal page",
    "dependencies": {
      "classnames": "2.2.6",
      "next": "10.2.0",
      "prop-types": "15.7.2",
      "react": "17.0.2",
      "react-dom": "17.0.2",
      "sass": "^1.32.13"
    },
    "scripts": {
      "dev": "next dev",
      "build": "next build && next export",
      "test": "echo \"Error: no test specified\" && exit 1",
      "start": "next start",
      "lint": "eslint ./components ./consts ./pages --ext .js --cache --ignore-pattern .gitignore",
      "lint:fix": "npm run lint -- --fix",
      "precommit": "npm run lint",
      "prepush": "npm run lint",
      "publish": "bash ./tools/publish.sh .temp"
    },
    "repository": {
      "type": "git",
      "url": "git+https://github.com/andres-kovalev/andres-kovalev.com.git"
    },
    "keywords": [
      "andres",
      "home-page"
    ],
    "author": {
      "name": "Andres Kovalev",
      "email": "mail@akovalev.ru"
    },
    "license": "MIT",
    "bugs": {
      "url": "https://github.com/andres-kovalev/andres-kovalev.com/issues"
    },
    "homepage": "https://andres-kovalev.com/",
    "devDependencies": {
      "babel-eslint": "^10.1.0",
      "eslint": "^7.26.0",
      "eslint-config-airbnb": "^18.1.0",
      "eslint-config-react-app": "^6.0.0",
      "eslint-plugin-flowtype": "^5.7.2",
      "eslint-plugin-import": "^2.22.1",
      "eslint-plugin-jsx-a11y": "^6.2.3",
      "eslint-plugin-react": "^7.19.0",
      "eslint-plugin-react-hooks": "^4.2.0",
      "husky": "^4.2.5",
      "postcss": "^8.2.15"
    },
    "eslintConfig": {
      "extends": "react-app"
    },
    "browserslist": {
      "production": [
        ">0.2%",
        "not dead",
        "not op_mini all"
      ],
      "development": [
        "last 1 chrome version",
        "last 1 firefox version",
        "last 1 safari version"
      ]
    },
    "husky": {
      "hooks": {
        "pre-commit": "npm run precommit",
        "pre-push": "npm run prepush"
      }
    }
  }
